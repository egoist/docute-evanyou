  "files": [
    "dist"
  ],
  "main": "dist/evanyou.js",
  "keywords": [
    "docute",
    "evanyou"
  ],
  "devDependencies": {
    "bili": "^0.10.0",
    "eslint": "latest",
    "eslint-config-rem": "latest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": "rem/esnext-browser"
  }
}
